{
  "name": "nadia-web-starter-kit",
  "version": "4.0.2",
  "scripts": {
    "start": "run-p serve dev",
    "dev": "run-s clean:dist watch",
    "build": "run-s clean:dist copy imagemin build:* cleancss",
    "build:webpack": "webpack --mode=production",
    "build:html": "run-p pug ejs",
    "clean:dist": "del-cli -f \"htdocs/**\" \"!htdocs/**/.*\" \"!htdocs/wp\" \"!htdocs/index.php\"",
    "watch": "run-p watch:*",
    "watch:webpack": "npm run webpack -- -w",
    "watch:pug": "onchange -i \"src/**/*.pug\" -- npm run pug",
    "watch:ejs": "onchange -i \"src/**/*.ejs\" -- npm run ejs",
    "watch:images": "onchange -i \"src/assets/images/**/*.{jpg,jpeg,png,gif,svg}\" -- npm run imagemin {{changed}}",
    "watch:copy": "onchange -i \"src/**/!(*.js|*.scss|*.pug|*.ejs|*.jpg|*.jpeg|*.png|*.gif|*.svg)\" -- npm run copy",
    "serve": "browser-sync start -c bs-config.js",
    "cleancss": "cleancss -o htdocs/assets/css/app.css htdocs/assets/css/app.css",
    "webpack": "webpack --mode=development",
    "pug": "pug src -o htdocs -P",
    "ejs": "ejs-cli --base-dir src \"/**/{_tpl_,!(_)}*.ejs\" -o htdocs",
    "imagemin": "node imagemin.js",
    "copy": "copyfiles -u 1 \"src/**/!(*.js|*.scss|*.pug|*.ejs|*.jpg|*.jpeg|*.png|*.gif|*.svg)\" htdocs"
  },
  "browserslist": [
    "last 2 version"
  ],
  "author": "nadia Inc.",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "autoprefixer": "^10.4.1",
    "babel-loader": "^8.2.3",
    "browser-sync": "^2.27.7",
    "clean-css-cli": "^5.5.0",
    "copyfiles": "^2.4.1",
    "core-js": "^3.20.2",
    "css-loader": "^6.7.3",
    "del-cli": "^4.0.1",
    "ejs-cli": "^2.2.3",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "html5-reset": "^2.1.3",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^9.0.0",
    "jquery": "^3.6.0",
    "mini-css-extract-plugin": "^2.6.1",
    "momentum-parallax": "^0.0.19",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.5.1",
    "pug-cli": "github:pugjs/pug-cli#master",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "volta": {
    "node": "16.13.1"
  }
}
